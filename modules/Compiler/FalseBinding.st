Class {
	#name : #FalseBinding,
	#superclass : #LiteralBinding,
	#category : #'Powerlang-Core-SCompiler-Bindings'
}

{ #category : #accessing }
FalseBinding >> encodeUsing: anAstcodeEmitter [
	anAstcodeEmitter encodeFalse
]

{ #category : #accessing }
FalseBinding >> name [
	^'false'
]

{ #category : #evaluating }
FalseBinding >> value [
	^false
]

{ #category : #visiting }
FalseBinding >> valueWithin: anEvaluationContext [
        ^ anEvaluationContext falseLiteral
]
